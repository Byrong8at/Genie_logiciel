<Application x:Class="Versionn_1._9_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Versionn_1._9_WPF"
             xmlns:viewModel="clr-namespace:Versionn_1._9_WPF.MVVM.ViewModel" 
             xmlns:view="clr-namespace:Versionn_1._9_WPF.MVVM.View">
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=viewModel:HomeViewModel}">
            <view:HomeView/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type TypeName=viewModel:LanguageViewModel}">
            <view:LanguageView/>
        </DataTemplate>
    </Application.Resources>
</Application>
